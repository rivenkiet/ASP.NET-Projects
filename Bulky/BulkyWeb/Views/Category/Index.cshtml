@model List<Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>
				Name Category
			</th>
			<th>
				Display Order
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var item in Model.OrderBy(i => i.DisplayOrder))
		{
			<tr>
				<th>
					@item.Name
				</th>
				<th>
					@item.DisplayOrder
				</th>
			</tr>
		}
	</tbody>
	
</table>